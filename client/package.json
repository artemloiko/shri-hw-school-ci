{
  "name": "school-ci-client",
  "description": "School CI. Layout.",
  "version": "1.0.0",
  "private": true,
  "author": "Artem Loiko",
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@bem-react/classname": "^1.5.8",
    "@reach/router": "^1.3.3",
    "ansi-to-html": "^0.6.14",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "date-fns": "^2.11.1",
    "formik": "^2.1.4",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-toast-notifications": "^2.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/reach__router": "^1.3.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-modal": "^3.10.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-toast-notifications": "^2.4.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cross-env": "^7.0.2",
    "fake-indexeddb": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "rollup": "^2.8.0",
    "rollup-plugin-terser": "^5.3.0",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^25.4.0",
    "tslib": "^1.11.2",
    "types-serviceworker": "0.0.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "sw:start": "rollup -w -c rollup.config.js",
    "sw:build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "sw:cleanup": "rm -rf temp",
    "pretest": "npm run sw:build",
    "prebuild": "npm run sw:build",
    "posttest": "npm run sw:cleanup",
    "postbuild": "npm run sw:cleanup",
    "client:sw": "cross-env REACT_APP_SW_DEVELOPMENT=true react-app-rewired start",
    "start:sw": "run-p sw:start client:sw",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint src --ext js,jsx,ts,tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/**/*.[jt]s?(x)",
          "**/?(*.)+(spec|test).[tj]s?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-empty-function": "off",
          "@typescript-eslint/no-explicit-any": "off"
        }
      }
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "engines": {
    "node": ">= 12.16.1",
    "npm": ">= 6.13.4"
  },
  "engineStrict": true
}
