{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "plugins": ["jest"],
  "extends": ["eslint-config-airbnb-base", "plugin:node/recommended", "plugin:jest/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": "off",
    "consistent-return": "off",
    "object-curly-newline": "off",
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": "warn"
  }
}
